{
  "name": "expo-starter-kit",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env EXPO_NO_DOTENV=1 npx expo start",
    "prebuild": "cross-env EXPO_NO_DOTENV=1 npx expo prebuild",
    "android": "cross-env EXPO_NO_DOTENV=1 npx expo run:android",
    "ios": "cross-env EXPO_NO_DOTENV=1 npx expo run:ios",
    "web": "cross-env EXPO_NO_DOTENV=1 npx expo start --web",
    "prebuild:development": "cross-env APP_ENV=development yarn prebuild",
    "start:staging": "cross-env APP_ENV=staging yarn start",
    "prebuild:staging": "cross-env APP_ENV=staging yarn prebuild",
    "android:staging": "cross-env APP_ENV=staging yarn android",
    "ios:staging": "cross-env APP_ENV=staging yarn ios",
    "start:production": "cross-env APP_ENV=production yarn start",
    "prebuild:production": "cross-env APP_ENV=production yarn prebuild",
    "android:production": "cross-env APP_ENV=production yarn android",
    "ios:production": "cross-env APP_ENV=production yarn ios",
    "build:development:ios": "cross-env APP_ENV=development EXPO_NO_DOTENV=1 SENTRY_AUTH_TOKEN=$SENTRY_AUTH_TOKEN eas build --profile development --platform ios",
    "build:development:android": "cross-env APP_ENV=development EXPO_NO_DOTENV=1 SENTRY_AUTH_TOKEN=$SENTRY_AUTH_TOKEN eas build --profile development --platform android",
    "build:staging:ios": "cross-env APP_ENV=staging EXPO_NO_DOTENV=1 SENTRY_AUTH_TOKEN=$SENTRY_AUTH_TOKEN eas build --profile staging --platform ios",
    "build:staging:android": "cross-env APP_ENV=staging EXPO_NO_DOTENV=1 SENTRY_AUTH_TOKEN=$SENTRY_AUTH_TOKEN eas build --profile staging --platform android",
    "build:production:ios": "yarn prebuild:production && cross-env APP_ENV=production EXPO_NO_DOTENV=1 SENTRY_AUTH_TOKEN=$SENTRY_AUTH_TOKEN eas build --profile production --platform ios",
    "build:production:android": "yarn prebuild:production && cross-env APP_ENV=production EXPO_NO_DOTENV=1 SENTRY_AUTH_TOKEN=$SENTRY_AUTH_TOKEN eas build --profile production --platform android",
    "update:staging": "cross-env APP_ENV=staging EXPO_NO_DOTENV=1 eas update --branch staging --message 'Staging OTA Update'",
    "update:production": "cross-env APP_ENV=production EXPO_NO_DOTENV=1 eas update --branch production --message 'Production OTA Update'",
    "test": "jest --watchAll",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo-google-fonts/noto-sans-thai": "^0.2.3",
    "@expo/react-native-action-sheet": "^4.1.1",
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/bottom-sheet": "^5.0.6",
    "@likashefqet/react-native-image-zoom": "^4.3.0",
    "@react-native-community/slider": "4.5.6",
    "@react-navigation/native": "^7.0.14",
    "@sentry/react-native": "~6.14.0",
    "@shopify/flash-list": "1.7.6",
    "@tabler/icons-react-native": "^3.26.0",
    "app-icon-badge": "^0.1.2",
    "axios": "^1.7.9",
    "bplist-creator": "^0.1.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "eventemitter3": "^5.0.1",
    "expo": "53.0.10",
    "expo-build-properties": "~0.14.6",
    "expo-camera": "~16.1.7",
    "expo-clipboard": "~7.1.4",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.0",
    "expo-device": "~7.1.4",
    "expo-file-system": "~18.1.10",
    "expo-font": "~13.3.1",
    "expo-image": "~2.2.0",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.5",
    "expo-localization": "~16.1.5",
    "expo-media-library": "~17.1.7",
    "expo-notifications": "~0.31.3",
    "expo-router": "~5.0.7",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.8",
    "expo-updates": "~0.28.14",
    "expo-web-browser": "~14.1.6",
    "i18next": "^24.2.0",
    "lodash": "^4.17.21",
    "lottie-react-native": "7.2.2",
    "moti": "^0.29.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-content-loader": "^7.0.2",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.2.0",
    "react-native": "0.79.3",
    "react-native-collapsible": "^1.6.2",
    "react-native-collapsible-tab-view": "^8.0.0",
    "react-native-edge-to-edge": "1.6.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-keyboard-controller": "^1.15.2",
    "react-native-markdown-display": "^7.0.2",
    "react-native-mmkv": "^3.0.2",
    "react-native-modal": "^13.0.1",
    "react-native-pager-view": "6.7.1",
    "react-native-paper": "^5.12.5",
    "react-native-progress": "^5.0.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-snackbar": "^2.8.0",
    "react-native-svg": "15.11.2",
    "react-native-tab-view": "^4.0.5",
    "react-native-web": "^0.20.0",
    "tailwind-merge": "^3.3.0",
    "tailwind-rn": "^4.2.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "3.4.17",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^16.0.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.13",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^19.1.0",
    "cross-env": "^7.0.3",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.7",
    "jest-styled-components": "^7.2.0",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "overrides": {
    "@expo/config-plugins": "9.0.0"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false,
        "exclude": [
          "react-native-collapsible-tab-view",
          "react-native-keyboard-aware-scroll-view",
          "react-native-modal",
          "@expo-google-fonts/noto-sans-thai",
          "@expo/config-plugins",
          "@tabler/icons-react-native",
          "bplist-creator",
          "i18next",
          "lodash",
          "tailwind-variants",
          "tailwindcss",
          "@likashefqet/react-native-image-zoom",
          "react-native-snackbar"
        ]
      }
    }
  }
}
